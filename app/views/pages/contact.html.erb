<div class="container">
	<div class="contact-page-wrapper">
		<div class="contact-page">
			<h1>Contact Us</h1>

			<form>

				<div class="form-group">
					<label for="fullname">Name*</label>
					<input type="text" class="form-control"  id="fullname" name="fullname" autofocus="true">
				</div>

				<div class="form-group">
					<label for="emailid">Email*</label>
					<input type="email" class="form-control"  id="emailid" name="emailid" autofocus="true">
				</div>

				<div class="form-group">
					<label for="subject">Subject*</label>
					<input type="text" class="form-control"  id="subject" name="subject" autofocus="true">
				</div>

				<div class="form-group">
					<label for="message">Message*</label>
					<input type="textarea" class="form-control"  id="message" name="message" autofocus="true">
				</div>	

				<div class="actions">
					<button type="submit" class="btn btn-primary">Send Message</button>
				</div>
			</form>
		</div>
	</div>
</div>
